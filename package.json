{
  "name": "multitrack-player",
  "version": "1.0.0",
  "description": "Professional MultiTrack Player with advanced audio mixing capabilities",
  "author": "MultiTrack Player Team",
  "main": "http://localhost:3000/judith/web-app.html",
  "node-remote": "http://localhost:3000",
  "chromium-args": "--disable-web-security",
  "window": {
    "title": "MultiTrack Player",
    "width": 1280,
    "height": 800,
    "show": true,
    "frame": true
  },
  "scripts": {
    "start": "node b2-proxy.js",
    "nw": "nw .",
    "electron": "electron .",
    "electron-dev": "electron . --dev",
    "build-app": "electron-builder",
    "build-win": "electron-builder --win",
    "build-mac": "electron-builder --mac",
    "build-linux": "electron-builder --linux",
    "dist": "npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "nw": "^0.107.0"
  },
  "private": true
}